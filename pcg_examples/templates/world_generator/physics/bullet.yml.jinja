engine: bullet
args:
  max_step_size: {{ max_step_size|default(0.001, true) }}
  real_time_factor: {{ real_time_factor|default(1, true) }}
  real_time_update_rate: {{ real_time_update_rate|default(1000, true) }}
  max_contacts: {{ max_contacts|default(20, true) }}
  min_step_size: {{ min_step_size|default(0.001, true) }}
  iters: {{ iters|default(50, true) }}
  sor: {{ sor|default(1.3, true) }}
  cfm: {{ cfm|default(0, true) }}
  erp: {{ erp|default(0.2, true) }}
  contact_surface_layer: {{ contact_surface_layer|default(0.001, true) }}
  split_impulse: {{ split_impulse|default(true, true) }}
  split_impulse_penetration_threshold: {{ split_impulse_penetration_threshold|default(-0.01, true) }}